#!/bin/sh
set -e
set -x

gcc -I.  -I"/secure/SecureNotes/Tcl_Evolution/jimsh"    -pipe -g3 -O0 -Wall -fPIC -std=c11 -c -o invoke.o  invoke.c


gcc -pipe -g3 -O0 -Wall -fPIC -std=c11 -Wl,--export-dynamic  -shared -o invoke.so  invoke.o  /usr/lib/x86_64-linux-gnu/libffi.a 


# ./jimsh  test.tcl

valgrind  -q  ./jimsh  test.tcl
